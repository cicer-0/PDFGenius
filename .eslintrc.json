{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "standard-with-typescript",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/semi": "off",
		"@typescript-eslint/indent": ["error", "2"],
		"no-tabs": "off"
	}
}
